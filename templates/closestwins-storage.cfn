---
AWSTemplateFormatVersion: "2010-09-09"
Description: Closest wins Storage v0.1.0

Parameters:
  Branch:
    Description: branch name
    Type: String

Resources:
  DatabaseStack:
    Type: AWS::CloudFormation::Stack
    Properties:
      TemplateURL: !Sub "https://closestwins-cfn-templates.s3.eu-west-1.amazonaws.com/closestwins-storage/${Branch}/database.cfn"
      TimeoutInMinutes: 10
      Parameters:
        Branch: !Ref Branch
